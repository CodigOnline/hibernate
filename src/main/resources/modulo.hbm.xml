<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="entities.Modulo" table="modulos">
        <id name="moduloId" type="long" column="modulo_id">
            <generator class="native"></generator>
        </id>
        <property name="nombre" column="nombre" type="string"></property>
        <property name="abreviatura" column="abreviatura" type="string"></property>
    </class>

</hibernate-mapping>